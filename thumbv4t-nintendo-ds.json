{
  "abi": "eabi",
  "arch": "arm",
  "data-layout": "e-m:e-p:32:32-Fi8-i64:64-v128:64:128-a:0:32-n32-S64",
  "env" : "newlib",
  "exe-suffix" : ".arm7_elf",
  "is-builtin": false,
  "linker": "arm-none-eabi-gcc",
  "llvm-target": "thumbv4t-none-eabi",
  "relocation-model": "static",
  "target-pointer-width": "32",
  "max-atomic-width": 32,
  "features" :"+crt-static,+soft-float,+strict-align,+atomics-32",
  "panic-strategy" : "abort",
  "target-family": [
      "unix"
    ],
  "no-default-libraries": false,
  "pre-link-args": {
      "gcc": [
        "-specs=ds_arm7.specs",
        "-g",
        "-mthumb-interwork",
        "-march=armv4t",
        "-mcpu=-mcpu=arm7tdmi",
        "-mthumb",
        "-mtune=arm7tdmi",
        "-fomit-frame-pointer",
        "-ffast-math"
      ]
    },
    "main-needs-argc-argv":"false",
  "vendor" : "nintendo",
  "os" : "nintendo_ds_arm7"
}

