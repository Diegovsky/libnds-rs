{
  "abi": "eabi",
  "arch": "arm",
  "data-layout": "e-S64-p:32:32-i64:64-m:e-n32",
  "env" : "newlib",
  "exe-suffix" : ".arm7.elf",
  "is-builtin": false,
  "linker": "arm-none-eabi-gcc",
  "llvm-target": "thumbv4t-none-eabi",
  "relocation-model": "static",
  "target-pointer-width": "32",
  "max-atomic-width": 32,
  "panic-strategy" : "abort",
  "target-family": [
      "unix"
    ],
  "no-default-libraries": false,
  "pre-link-args": {
      "gcc": [
        "-specs=ds_arm7.specs",
        "-g",
        "-march=armv4t",
        "-mthumb",
        "-mthumb-interwork",
        "-mtune=arm7tdmi",
        "-fomit-frame-pointer",
        "-ffast-math",
        "-mlittle-endian"
      ]
    },
    "has-elf-tls":"false",
    "main-needs-argc-argv":"false",
  "vendor" : "nintendo"
}

